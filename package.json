{
  "name": "oblique-galaxy",
  "version": "0.0.1",
  "scripts": {
    "start": "npm run check-updates && ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "check-updates": "npm outdated --json > projects/sandbox/src/assets/outdated-report.json || true",
    "test": "ng test",
    "lint": "npm run format && ng lint && npm run repository-lint",
    "repository-lint": "./scripts/base-folder-lint.sh && ./scripts/codeowner-lint.sh",
    "format": "./node_modules/.bin/prettier . --check",
    "prepare": "husky"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.9",
    "@angular/cdk": "^17.3.9",
    "@angular/common": "^17.3.9",
    "@angular/compiler": "^17.3.9",
    "@angular/core": "^17.3.9",
    "@angular/forms": "^17.3.9",
    "@angular/material": "^17.3.9",
    "@angular/platform-browser": "^17.3.9",
    "@angular/platform-browser-dynamic": "^17.3.9",
    "@angular/router": "^17.3.9",
    "@angular/service-worker": "^17.3.9",
    "@ngx-translate/core": "^15.0.0",
    "@oblique/oblique": "^11.1.2",
    "@popperjs/core": "^2.11.8",
    "angular-oauth2-oidc": "^17.0.2",
    "jwt-decode": "^4.0.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.6"
  },
  "devDependencies": {
    "@angular-builders/jest": "^17.0.3",
    "@angular-devkit/build-angular": "^17.3.7",
    "@angular-eslint/builder": "17.4.1",
    "@angular-eslint/eslint-plugin": "17.4.1",
    "@angular-eslint/eslint-plugin-template": "17.4.1",
    "@angular-eslint/schematics": "17.4.1",
    "@angular-eslint/template-parser": "17.4.1",
    "@angular/cli": "^17.3.7",
    "@angular/compiler-cli": "^17.3.9",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "7.8.0",
    "@typescript-eslint/parser": "7.8.0",
    "browser-sync": "^3.0.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ng-packagr": "^17.3.0",
    "prettier": "^3.2.5",
    "typescript": "~5.4.5"
  },
  "jestSonar": {
    "reportPath": "./coverage/sonarQube",
    "reportFile": "sqr.xml",
    "indent": 4,
    "sonar56x": true
  }
}
